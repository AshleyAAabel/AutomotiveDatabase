<html>
<head>
    <title>Customers Search</title>
</head>
<body>
    <h1>Customers with Last Name {{last}}</h1>
    [ <a href="/">Home</a> |  <a href="/cars">Cars</a> | <a href="/recalls">Recalls</a> | <a href="/cars_recalls">Cars and Recalls<a/> | <a href="/customers">Customers</a> | <a href="/customers_vehicles">Customers' Vehicles</a> | <a href="/vehicle_recall_status">Vehicle Recall Status</a>]
    <table border="1" cellpadding="5">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone Number</th>
        <th>Email</th>
        <th></th>
        <th></th>

    </tr>
    <tbody>
    {% for customer in customers %}
    <tr>
        <td>{{customer['firstName']}}</td>
        <td>{{customer['lastName']}}</td>
        <td>{{customer['phoneNumber']}}</td>
        <td>{{customer['email']}}</td>
        <td><a href="edit_customer/{{customer.customerID}}">Edit</a></td>
        <td><a href="view_customers_vehicles/{{customer.customerID}}">View all Vehicles</a></td>
    </tr>
    {% endfor %}
    </table>



</body>
</html>