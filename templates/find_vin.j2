<html>
<head>
    <title>Customers' Vehicles</title>
</head>
<body>
    <h1>Customers' Vehicles</h1>
    [ <a href="/">Home</a> |  <a href="/cars">Cars</a> | <a href="/recalls">Recalls</a> | <a href="/cars_recalls">Cars and Recalls<a/> | <a href="/customers">Customers</a> | <a href="/customers_vehicles">Customers' Vehicles</a> | <a href="/vehicle_recall_status">Vehicle Recall Status</a>]]
    <table border="1" cellpadding="5">
    <thead>
    <tr>
        <th>Customer First Name</th>
        <th>Customer Last Name</th>
        <th>Customer Phone Number</th>
        <th>Car Make</th>
        <th>Car Model</th>
        <th>Model Year</th>
        <th>VIN</th>
        <th>Sale Date</th>
        <th>Last Service Date</th>
        <th></th>
        <th></th>
        <th></th>

    </tr>
    <tbody>
    {% for customersVehicle in customersVehicles %}
    <tr>
        <td>{{customersVehicle['firstName']}}</td>
        <td>{{customersVehicle['lastName']}}</td>
        <td>{{customersVehicle['phoneNumber']}}</td>
        <td>{{customersVehicle['carMake']}}</td>
        <td>{{customersVehicle['carModel']}}</td>
        <td>{{customersVehicle['carYear']}}</td>
        <td>{{customersVehicle['vinNumber']}}</td>
        <td>{{customersVehicle['saleDate']}}</td>
        <td>{{customersVehicle['lastServiceDate']}}</td>
        <td><a href="/delete_customers_vehicle/{{customersVehicle.customerVehicleID}}">Delete</a></td>
        <td><a href="/edit_customers_vehicle/{{customersVehicle.customerVehicleID}}">Update Service Date</a></td>
        <td><a href="/edit_customers_vehicle_customer/{{customersVehicle.customerVehicleID}}">Edit Customer</a></td>
    </tr>
    {% endfor %}
    </table>
</body>
</html>